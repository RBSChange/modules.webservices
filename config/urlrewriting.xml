<?xml version="1.0" encoding="UTF-8"?>
<urlrewriting>
	<rules>
		<rule redirection="webservices/Server">
			<template>/webservices/$moduleName/$serviceName</template>
		</rule>
		<rule redirection="webservices/GetSchema">
			<template>/webservices-schemas/$moduleName/$serviceName</template>
		</rule>
		<rule redirection="webservices/ServerJSON">
			<template>/servicesjson/$moduleName/$serviceName</template>
		</rule>
	</rules>
</urlrewriting>